var AdminUsers = {
  init : function() {
        
    $("a#promote").click(function() {
      return confirm(TRANSLATIONS['PROMOTE_TO_ADMIN']);
    });
    
    $("a#demote").click(function() {
      return confirm(TRANSLATIONS['DEMOTE_FROM_ADMIN']);
    });
    
    $("a.remove_user").click(function() {
      return confirm(TRANSLATIONS['REMOVE_USER']);
    });
  }
  
}

$(AdminUsers.init);